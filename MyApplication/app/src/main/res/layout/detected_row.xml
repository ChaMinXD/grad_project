<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="50dp"
    android:layout_marginTop="20dp"
    xmlns:app="http://schemas.android.com/apk/res-auto">
    
    <View
        android:id="@+id/row_View"
        android:layout_width="match_parent"
        android:layout_height="50dp"
        android:background="@drawable/round_shape"
        android:layout_marginStart="10dp"
        android:layout_marginEnd="10dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toEndOf="parent" />
    <androidx.constraintlayout.utils.widget.ImageFilterView
        android:layout_width="30dp"
        android:layout_height="30dp"
        android:id="@+id/row_icon"
        android:background="@drawable/warn_icon"
        android:layout_marginStart="5dp"
        app:layout_constraintTop_toTopOf="@id/row_View"
        app:layout_constraintBottom_toBottomOf="@id/row_View"
        app:layout_constraintStart_toStartOf="@+id/row_View"/>
    <TextView
        android:layout_width="200dp"
        android:layout_height="30dp"
        android:textSize="16sp"
        android:textAlignment="center"
        android:id="@+id/row_vaccine"
        android:text="라라라라"
        android:textColor="#194185"
        app:layout_constraintStart_toEndOf="@id/row_icon"
        app:layout_constraintTop_toTopOf="@id/row_View"
        app:layout_constraintBottom_toBottomOf="@id/row_View"
        />
    <View
        android:layout_width="1dp"
        android:layout_height="50dp"
        android:background="#194185"
        app:layout_constraintTop_toTopOf="@id/row_View"
        app:layout_constraintBottom_toBottomOf="@id/row_View"
        app:layout_constraintStart_toEndOf="@id/row_vaccine"
        app:layout_constraintEnd_toStartOf="@id/row_result"
        />
    <TextView
        android:layout_width="100dp"
        android:layout_height="30dp"
        android:textSize="16sp"
        android:textAlignment="center"
        android:id="@+id/row_result"
        android:text="라라라라"
        android:textColor="#194185"
        app:layout_constraintStart_toEndOf="@id/row_vaccine"
        app:layout_constraintTop_toTopOf="@id/row_View"
        app:layout_constraintBottom_toBottomOf="@id/row_View"
        app:layout_constraintEnd_toEndOf="@id/row_View"
        />


</androidx.constraintlayout.widget.ConstraintLayout>